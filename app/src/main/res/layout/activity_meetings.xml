<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/queenb_pink"
    tools:context=".Meetings">

    <TextView
        android:id="@+id/meetings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Meetings"
        android:textAlignment="center"
        android:textColor="@color/queenb_text_gray"
        android:textColorHighlight="@color/queenb_text_gray"
        android:textColorHint="@color/queenb_text_gray"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="60dp"
        android:layout_height="4dp"
        android:background="#FF0000FF"
        android:backgroundTint="@color/queenb_text_gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/meetings"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>